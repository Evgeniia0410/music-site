CREATE TABLE IF NOT EXISTS GANR (
   id SERIAL PRIMARY KEY,
   name_ganr VARCHAR (80) NOT NULL
 );
 CREATE TABLE IF NOT EXISTS EXECUTOR (
   id SERIAL PRIMARY KEY,
   name_executor VARCHAR (80) NOT NULL
 );
 CREATE TABLE IF NOT EXISTS GANR-EXECUTOR (
   GANR_id int REFERENCES GANR (id),
   EXECUTOR_id int REFERENCES EXECUTOR (id),
   CONSTRAINT GANR_EXECUTOR_pk PRIMARY KEY (GANR_id, EXECUTOR_id)
 );
 CREATE TABLE IF NOT EXISTS ALBUM (
   id SERIAL PRIMARY KEY,
   name_album VARCHAR (80) NOT NULL,
   year_of_release int NOT NULL
 );
 CREATE TABLE IF NOT EXISTS EXECUTOR-ALBUM (
   ALBUM_id int REFERENCES ALBUM (id),
   EXECUTOR_id int REFERENCES EXECUTOR (id),
   CONSTRAINT EXECUTOR_ALBUM_pk PRIMARY KEY (ALBUM_id, EXECUTOR_id)
   );
 CREATE TABLE IF NOT EXISTS TRACK (
   id SERIAL PRIMARY KEY,
   name_track VARCHAR (80) NOT NULL,
   duration int NOT NULL,
   fk.track-Album int REFERENCES ALBUM (id)
  );  
  CREATE TABLE IF NOT EXISTS COLLECTION (
   id SERIAL PRIMARY KEY,
   name_collection VARCHAR (90) NOT NULL,
   year_of_release int NOT NULL
  );  
  CREATE TABLE IF NOT EXISTS TRACK_COLLECTION (
   COLLECTION_id int REFERENCES COLLECTION (id),
   TRACK_id int REFERENCES TRACK (id),
   CONSTRAINT TRACK_COLLECTION_pk PRIMARY KEY (COLLECTION_id, TRACK_id)
   );
